%%stdout%%
func: ()
	match this
		case Nil
			Nil
		case Cons (_, Nil)
			this
		case Cons (@hd, @tl)
			private const l, r = this.split (Nil, Nil)
			l.sort ().merge (r.sort ())
	end
end
func: f
	match this
		case Nil
			Nil
		case Cons (@hd, @tl)
			Cons (f hd, tl.map f)
	end
end
func: f
	this.reduce (Nil, (func: base, val
		given (f val)
			case true
				Cons (val, base)
			case false
				base
		end
	end))
end
func: n
	given n
		case 0
			this.head
		case 1
			this.tail
		default
			throw (ArgumentError.new ("Argument must be 0 or 1", "List.operator->", n))
	end
end
%%stderr%%
